{
  "name": "@sky-mavis/headless",
  "repository": {
    "url": "https://github.com/skymavis/headless-js"
  },
  "version": "0.0.2",
  "license": "MIT",
  "publishConfig": {
    "access": "private"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "*",
    "!**/*.test.ts",
    "vitest.config.ts",
    "!**/*.tsbuildinfo",
    "!tsconfig.build.json",
    "!rollup.config.mjs"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./common": {
      "types": "./dist/types/common/index.d.ts",
      "import": "./dist/module/common/index.js",
      "require": "./dist/commonjs/common/index.js"
    },
    "./v1": {
      "types": "./dist/types/v1/index.d.ts",
      "import": "./dist/module/v1/index.js",
      "require": "./dist/commonjs/v1/index.js"
    },
    "./v2": {
      "types": "./dist/types/v2/index.d.ts",
      "import": "./dist/module/v2/index.js",
      "require": "./dist/commonjs/v2/index.js"
    }
  },
  "scripts": {
    "gen-v": "genversion --esm --double --property name,version common/version.ts",
    "build": "rm -rf dist && pnpm gen-v && rollup -c",
    "dev": "rm -rf dist && rollup -c --watch",
    "lint": "eslint . --ext .js,.ts --max-warnings=0",
    "clean": "rm -rf .turbo node_modules dist",
    "test": "vitest run --typecheck"
  },
  "devDependencies": {
    "@babel/preset-env": "7.24.4",
    "@bufbuild/protobuf": "2.2.2",
    "@noble/curves": "1.6.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@sky-mavis/eslint-config-ts": "workspace:*",
    "@types/node": "20.12.11",
    "eslint": "8.57.0",
    "genversion": "3.2.0",
    "glob": "11.0.0",
    "jsdom": "25.0.1",
    "jwt-decode": "4.0.0",
    "prettier": "3.2.5",
    "rollup": "4.14.1",
    "rollup-plugin-generate-package-json": "3.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-polyfill-node": "0.13.0",
    "rollup-plugin-typescript2": "0.36.0",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "uuid": "11.0.4",
    "viem": "2.21.22",
    "vitest": "2.1.4"
  }
}
